{
  "version": "1.8.0",

  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalAsyncFunctions": true,
    "experimentalDecorators": true,
    "listFiles": true,
    "module": "system",
    "moduleResolution": "node",
    "noEmitHelpers": false,
    "noImplicitAny": false,
    "noLib": false,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5"
  }
}
